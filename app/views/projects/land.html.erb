<div class="container">
  <%= image_tag "land.jpg" %>
</div>
<%- model_class = Project -%>
<div class="container">
<div class="page-header">
  <h1>Land</h1>
</div>
</div>
  <%@projects.each do |land|%>
  <div class="container landprofile">
    <div class="row">

      <div class="span3" data-no-turbolink>
        <%= link_to image_tag(land.url,:class => 'headimg'),userland_division_path(land.id)%>
      </div>
      <div class="span9" data-no-turbolink>
        <h2><%= link_to land.name, userland_division_path(land.id) %></h2>
        <p><%= land.description %></p>
      </div>
  </div>
  </div>
  <hr>
  <%end%>