<%- model_class = Homeland -%>
<div class="container">
<div class="page-header">
  <h1><%= @project.name %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:lot) %></th>
      <th><%= model_class.human_attribute_name(:address) %></th>
      <th><%= model_class.human_attribute_name(:size) %></th>
      <th><%= model_class.human_attribute_name(:bedroom) %></th>
      <th><%= model_class.human_attribute_name(:bathroom) %></th>
      <th><%= model_class.human_attribute_name(:garage) %></th>
      <th><%= model_class.human_attribute_name(:price) %></th>
      <th><%= model_class.human_attribute_name(:status) %></th>      
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @homes.each do |home| %>
      <tr>
        <td><%= home.lot %></td>
        <td><%= home.address %></td>
        <td><%= home.size.to_i %></td>
        <td><%= home.bedroom %></td>
        <td><%= home.bathroom %></td>
        <td><%= home.garage %></td>
        <td><%= home.price.to_i %></td>
        <td><%= home.status %></td>
        <td>
          <%= link_to "View detail",newhome_path(home), :class => 'btn btn-mini' %>

        </td>
      </tr>
    <% end %>
  </tbody>
</table>

</div>